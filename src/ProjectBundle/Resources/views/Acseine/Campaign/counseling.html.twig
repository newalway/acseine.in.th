                                        <input type="checkbox" name="counseling_register[know_acseine_yes][]" id="know_acseine_yes_{{loop.index}}" value="{{know_acseine_yes}}" ng-model="formData.param.know_acseine_yes['{{know_acseine_yes}}']" ng-disabled="formData.param.know_acseine == 'ไม่รู้จัก'"> {{know_acseine_yes}}
