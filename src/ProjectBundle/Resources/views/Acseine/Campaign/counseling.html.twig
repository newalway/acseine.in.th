<script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>

<script>
    var counselingApp = angular.module('counselingApp', []);
    counselingApp.factory('counseling_data', ['$http', function counseling_data($http){
        return {
            create: function(dataObj){
                return $http({
                    method:'POST',
                    url: '{{campaign_api_url}}',
                    data: $.param(dataObj),
                    headers: {
                        'Authorization':'{{campaign_api_authorization}}',
                        'Content-Type':'application/x-www-form-urlencoded; charset=UTF-8'
                    }
                });
            }
        }
    }]);

    counselingApp.controller('counselingController', ['$scope', '$sce', 'counseling_data', function($scope, $sce, counseling_data) {
        $scope.getDataLoading = function() {
            return $scope.data_loaded;
        };
        $scope.setDataLoading = function(value) {
            $scope.data_loaded = value;
        };

        $scope.resetKnow= function(){
            $scope.formData.param.know_acseine_yes = [];
            $scope.formData.param.know_acseine_yes_other = '';
        }

        $scope.processForm = function() {
            $scope.setDataLoading(true);
            counseling_data.create($scope.formData).then(function onSuccess(response) {
                var data = response.data;console.log(data);
                if (!data.success) {
                    $scope.message = data.message;
                    $scope.errors = data.error_message
                    $scope.success = data.success;
                } else {
                    $scope.message = data.message;
                    //reset data
                    $scope.formData = {};
                    $scope.errors = {};
                    $scope.success = {};
                }
                $scope.setDataLoading(false);
            }).catch(function onError(response) {
                console.log(response);
                $scope.setDataLoading(false);
            });
        };

        $scope.formData = {};
        $scope.errors = {};
        $scope.message = null;
        $scope.setDataLoading(false);
    }]);
</script>
{% endblock %}
