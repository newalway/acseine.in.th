                                <div class="text-error ng-cloak" ng-show="errors.know_acseine_yes_other">{{'{{errors.know_acseine_yes_other}}'}}</div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 control-label">คุณอายุเท่าไร?</label>
                            <div class="col-sm-9 col-md-5">
                                {% set age_range_data = ['14 - 18', '19 - 25', '26 - 35', '36 - 45', '45+'] %}
                                <select name="counseling_register[age_range]" class="form-control" ng-model="formData.param.age_range">
                                    <option value="" selected>--- เลือกช่วงอายุ ---</option>
                                    {% for age_range in age_range_data %}
                                    <option value="{{age_range}}">{{age_range}}</option>
